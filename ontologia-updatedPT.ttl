# --- Outras Regras de Adaptação e Seus Componentes (Exemplo Adicional) ---

:SGP_StudentToColleagueCollaboration rdf:type :StyleGuidanceProfile ;
    rdfs:label "Perfil de Estilo: Estudante para Colega (Colaboração Projeto)"@pt ;
    :includesToneElement <http://www.semanticweb.org/ontologies/2025/5/academicEmailPersona#Tone_ColaborativoInformalTecnico> ; # Novo Tom a definir
    :specifiesFormalityElement <http://www.semanticweb.org/ontologies/2025/5/academicEmailPersona#Formality_BaixaMedia> ; # Nova Formalidade a definir
    :hasPrimaryCommunicationGoal :Goal_CollaborateEffectively .

:Tone_ColaborativoInformalTecnico rdf:type :ToneElement ;
    rdfs:label "Tom Colaborativo Informal Técnico"@pt ;
    :label "ColaborativoInformalTecnico" ;
    :toneKeyword "colaborativo"@pt, "direto"@pt, "partilha de informação"@pt, "linguagem técnica clara"@pt, "amigável"@pt ;
    :avoidToneKeyword "excessivamente formal"@pt, "passivo-agressivo"@pt, "muito vago"@pt ;
    :toneIntensity "0.5"^^xsd:float .

:Formality_BaixaMedia rdf:type :FormalityElement ;
    rdfs:label "Formalidade Baixa-Média"@pt ;
    :label "BaixaMedia" ;
    :formalityLevelNumeric "2"^^xsd:integer ;
    :formalityGuidanceNotes "Comunicação entre colegas. Pode ser informal, mas manter respeito e clareza, especialmente em discussões técnicas."@pt .

:rule_student_to_colleague_project_collaboration_do1 rdf:type :CommunicationGuideline;
    rdfs:label "S2C Collab Do 1"@pt;
    :guidelineText "Partilhar informação e recursos de forma clara e atempada."@pt .
:rule_student_to_colleague_project_collaboration_dont1 rdf:type :CommunicationGuideline;
    rdfs:label "S2C Collab Don't 1"@pt;
    :guidelineText "Não cumprir com as responsabilidades e prazos acordados."@pt .

:rule_student_to_colleague_project_collaboration rdf:type :GenericRecipientAdaptationRule ;
    rdfs:label "Regra: Estudante para Colega (Colaboração em Projeto)"@pt ;
    :ruleKey "student_to_colleague_project_collaboration" ;
    :description "Comunicação entre estudantes colegas de equipa em projetos académicos."@pt ;
    :greetingTemplate "Olá [PrimeiroNomeColega], / Boas equipa," ;
    :farewellTemplate "Abraço,\n[PrimeiroNomeAluno] / Cumps," ;
    :hasAdaptedStyleProfile :SGP_StudentToColleagueCollaboration ;
    :hasSpecificDo :rule_student_to_colleague_project_collaboration_do1 ;
    :hasSpecificDont :rule_student_to_colleague_project_collaboration_dont1 .


