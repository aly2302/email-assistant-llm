@prefix : <http://www.semanticweb.org/ontologies/2025/5/academicEmailPersona#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

#################################################################
#    Metadados da Ontologia
#################################################################

<http://www.semanticweb.org/ontologies/2025/5/academicEmailPersona> rdf:type owl:Ontology ;
    owl:versionIRI <http://www.semanticweb.org/ontologies/2025/5/academicEmailPersona/2.1.0-pt> ; # Versão incrementada
    rdfs:label "Ontologia de Personas para Email Académico (Completa V2.1)"@pt ;
    rdfs:comment """Meta-ontologia para a definição de personas de assistentes de email no contexto académico.
Esta versão V2.1.0-pt inclui estruturas avançadas para orientação de tom/estilo, uma base de conhecimento aprendida com contexto detalhado, e indivíduos de exemplo mais completos, com anotações em Português de Portugal."""@pt ;
    owl:versionInfo "2.1.0-pt" .

#################################################################
#    Propriedades de Anotação
#################################################################

:schema_version rdf:type owl:AnnotationProperty ;
    rdfs:label "Versão do Esquema Original"@pt ;
    rdfs:comment "Versão do esquema JSON original no qual esta ontologia OWL se baseia."@pt .

:notes_on_structure rdf:type owl:AnnotationProperty ;
    rdfs:label "Notas sobre a Estrutura"@pt ;
    rdfs:comment "Notas explicativas sobre a estrutura ou elementos, derivadas do JSON de origem."@pt .

:archetype_description_note rdf:type owl:AnnotationProperty ;
    rdfs:label "Nota de Descrição do Arquétipo"@pt .

:options_description rdf:type owl:AnnotationProperty ;
    rdfs:label "Descrição das Opções"@pt .

:applies_from_archetype_note rdf:type owl:AnnotationProperty ;
    rdfs:label "Nota de Aplicação a Partir do Arquétipo"@pt .

:applies_to_recipient_category_note rdf:type owl:AnnotationProperty ;
    rdfs:label "Nota de Aplicação à Categoria de Destinatário"@pt .

#################################################################
#    Classes (Estrutura de classes permanece a mesma da V2)
#################################################################

:PersonaArchetype rdf:type owl:Class ;
    rdfs:label "Arquétipo de Persona"@pt ;
    rdfs:comment "Um modelo ou padrão base para uma persona académica. Define características gerais e estilos de comunicação."@pt .
:StudentArchetype rdf:type owl:Class ;
    rdfs:subClassOf :PersonaArchetype ;
    owl:disjointWith :ProfessorArchetype ;
    rdfs:label "Arquétipo de Estudante"@pt ;
    rdfs:comment "Representa o arquétipo para uma persona de estudante."@pt .
:ProfessorArchetype rdf:type owl:Class ;
    rdfs:subClassOf :PersonaArchetype ;
    rdfs:label "Arquétipo de Professor"@pt ;
    rdfs:comment "Representa o arquétipo para uma persona de professor/docente."@pt .
:StyleGuidanceProfile rdf:type owl:Class ;
    rdfs:label "Perfil de Orientação de Estilo"@pt ;
    rdfs:comment "Um perfil abrangente que detalha vários aspetos do estilo de comunicação, incluindo tom, formalidade e objetivos gerais."@pt .
:ToneElement rdf:type owl:Class ;
    rdfs:label "Elemento de Tom"@pt ;
    rdfs:comment "Descreve uma qualidade tonal específica, incluindo palavras-chave para a alcançar e palavras-chave a evitar."@pt .
:FormalityElement rdf:type owl:Class ;
    rdfs:label "Elemento de Formalidade"@pt ;
    rdfs:comment "Descreve o nível e a natureza da formalidade requerida."@pt .
:CommunicationGoal rdf:type owl:Class ;
    rdfs:label "Objetivo de Comunicação"@pt ;
    rdfs:comment "Representa um objetivo primário para uma comunicação."@pt .
:CommunicationAttributes rdf:type owl:Class ;
    rdfs:label "Atributos de Comunicação"@pt ;
    rdfs:comment "Um conjunto de atributos que definem o estilo de comunicação base de um arquétipo de persona."@pt .
:CommunicationGuideline rdf:type owl:Class ;
    rdfs:label "Diretriz de Comunicação"@pt ;
    rdfs:comment "Uma diretriz específica de 'Fazer' ou 'Não Fazer' para a comunicação."@pt .
:GenericRecipientAdaptationRule rdf:type owl:Class ;
    rdfs:label "Regra Genérica de Adaptação ao Destinatário"@pt ;
    rdfs:comment "Define uma regra genérica para adaptar o estilo de comunicação com base na categoria do destinatário."@pt .
:LearnedKnowledgeItem rdf:type owl:Class ;
    rdfs:label "Item de Conhecimento Aprendido"@pt ;
    rdfs:comment "Um item de conhecimento aprendido a partir do feedback do utilizador."@pt .
:InteractionContextSnapshot rdf:type owl:Class ;
    rdfs:label "Instantâneo do Contexto da Interação"@pt ;
    rdfs:comment "Um instantâneo do contexto relevante no momento em que um feedback foi fornecido."@pt .
:IAProcessDefaultSet rdf:type owl:Class ;
    rdfs:label "Conjunto de Predefinições de Processo de IA"@pt ;
    rdfs:comment "Um conjunto de preferências padrão sobre como a IA deve operar."@pt .
:IAProcessOption rdf:type owl:Class;
    rdfs:label "Opção de Processo de IA"@pt ;
    rdfs:comment "Representa uma estratégia ou preferência de processo de IA configurável."@pt .
:AcademicEmailCategory rdf:type owl:Class ;
    rdfs:label "Categoria de Email Académico"@pt ;
    rdfs:comment "Representa uma categoria definida de emails académicos."@pt .

#################################################################
#    Propriedades de Objeto (Estrutura de propriedades permanece a mesma da V2)
#################################################################
:hasCommunicationAttributes rdf:type owl:ObjectProperty ; rdfs:label "possui Atributos de Comunicação"@pt ; rdfs:domain :PersonaArchetype ; rdfs:range :CommunicationAttributes .
:hasBaseStyleProfile rdf:type owl:ObjectProperty ; rdfs:label "possui Perfil de Estilo Base"@pt ; rdfs:domain :PersonaArchetype ; rdfs:range :StyleGuidanceProfile .
:hasGeneralDo rdf:type owl:ObjectProperty ; rdfs:label "possui 'Fazer' Geral"@pt ; rdfs:domain :PersonaArchetype ; rdfs:range :CommunicationGuideline .
:hasGeneralDont rdf:type owl:ObjectProperty ; rdfs:label "possui 'Não Fazer' Geral"@pt ; rdfs:domain :PersonaArchetype ; rdfs:range :CommunicationGuideline .
:hasRelevantGenericRule rdf:type owl:ObjectProperty ; rdfs:label "possui Regra Genérica Relevante"@pt ; rdfs:domain :PersonaArchetype ; rdfs:range :GenericRecipientAdaptationRule .
:hasLearnedItem rdf:type owl:ObjectProperty ; rdfs:label "possui Item Aprendido"@pt ; rdfs:domain :PersonaArchetype ; rdfs:range :LearnedKnowledgeItem .
:includesToneElement rdf:type owl:ObjectProperty ; rdfs:label "inclui Elemento de Tom"@pt ; rdfs:domain :StyleGuidanceProfile ; rdfs:range :ToneElement .
:specifiesFormalityElement rdf:type owl:ObjectProperty ; rdfs:label "especifica Elemento de Formalidade"@pt ; rdfs:domain :StyleGuidanceProfile ; rdfs:range :FormalityElement .
:hasPrimaryCommunicationGoal rdf:type owl:ObjectProperty ; rdfs:label "possui Objetivo de Comunicação Primário"@pt ; rdfs:domain :StyleGuidanceProfile ; rdfs:range :CommunicationGoal .
:hasAdaptedStyleProfile rdf:type owl:ObjectProperty ; rdfs:label "possui Perfil de Estilo Adaptado"@pt ; rdfs:domain :GenericRecipientAdaptationRule ; rdfs:range :StyleGuidanceProfile .
:hasSpecificDo rdf:type owl:ObjectProperty ; rdfs:label "possui 'Fazer' Específico"@pt ; rdfs:domain :GenericRecipientAdaptationRule ; rdfs:range :CommunicationGuideline .
:hasSpecificDont rdf:type owl:ObjectProperty ; rdfs:label "possui 'Não Fazer' Específico"@pt ; rdfs:domain :GenericRecipientAdaptationRule ; rdfs:range :CommunicationGuideline .
:hasInteractionContext rdf:type owl:ObjectProperty ; rdfs:label "possui Contexto da Interação"@pt ; rdfs:domain :LearnedKnowledgeItem ; rdfs:range :InteractionContextSnapshot .
:snapshotForPersona rdf:type owl:ObjectProperty ; rdfs:label "instantâneo para Persona"@pt ; rdfs:domain :InteractionContextSnapshot ; rdfs:range :PersonaArchetype .
:snapshotIdentifiedEmailCategory rdf:type owl:ObjectProperty ; rdfs:label "instantâneo Categoria de Email Identificada"@pt ; rdfs:domain :InteractionContextSnapshot ; rdfs:range :AcademicEmailCategory .
:definesHandlingOfMissingInfo rdf:type owl:ObjectProperty ; rdfs:label "define Tratamento de Info Crítica Em Falta"@pt ; rdfs:domain :IAProcessDefaultSet ; rdfs:range :IAProcessOption .
:definesPriorityOfUserInput rdf:type owl:ObjectProperty ; rdfs:label "define Prioridade do Input do Utilizador"@pt ; rdfs:domain :IAProcessDefaultSet ; rdfs:range :IAProcessOption .
:definesExpectedIAAutonomyLevel rdf:type owl:ObjectProperty ; rdfs:label "define Nível de Autonomia Esperado da IA"@pt ; rdfs:domain :IAProcessDefaultSet ; rdfs:range :IAProcessOption .
:definesBehaviorForUnclearRequests rdf:type owl:ObjectProperty ; rdfs:label "define Comportamento para Pedidos Pouco Claros"@pt ; rdfs:domain :IAProcessDefaultSet ; rdfs:range :IAProcessOption .
:definesEmailCategory rdf:type owl:ObjectProperty ; rdfs:label "define Categoria de Email"@pt ; rdfs:domain :IAProcessDefaultSet; rdfs:range :AcademicEmailCategory .

#################################################################
#    Propriedades de Dados (Estrutura de propriedades permanece a mesma da V2)
#################################################################
