#################################################################
#    Indivíduos (Agora mais completos)
#################################################################

# --- Indivíduos para Elementos de Orientação de Estilo (Ideia 1) ---

:Goal_ProvideClearUpdate rdf:type :CommunicationGoal ;
    rdfs:label "Objetivo de Fornecer Atualização Clara"@pt ;
    :label "ProvideClearUpdateAndAddressQueries" ;
    :goalDescription "O objetivo primário é fornecer uma atualização clara e concisa sobre o progresso, abordar quaisquer pontos pendentes e solicitar feedback ou decisões necessárias."@pt .

:Goal_RequestClarification rdf:type :CommunicationGoal ;
    rdfs:label "Objetivo de Pedir Esclarecimento"@pt ;
    :label "RequestClarification" ;
    :goalDescription "O objetivo primário é solicitar esclarecimentos sobre um tópico específico, dúvida ou informação."@pt .

:Goal_CollaborateEffectively rdf:type :CommunicationGoal ;
    rdfs:label "Objetivo de Colaborar Eficazmente"@pt ;
    :label "CollaborateEffectively" ;
    :goalDescription "O objetivo primário é facilitar a colaboração, partilhar informação e coordenar esforços com colegas ou parceiros."@pt .

:Tone_SemiFormalColaborativo rdf:type :ToneElement ;
    rdfs:label "Tom Semiformal Colaborativo"@pt ;
    :label "SemiFormalColaborativo" ;
    :toneKeyword "profissional mas acessível"@pt, "colaborativo"@pt, "claro"@pt, "construtivo"@pt, "orientado para soluções"@pt ;
    :avoidToneKeyword "excessivamente formal"@pt, "demasiado casual"@pt, "brusco"@pt, "vago"@pt ;
    :toneIntensity "0.6"^^xsd:float .

:Formality_Media rdf:type :FormalityElement ;
    rdfs:label "Formalidade Média"@pt ;
    :label "Media" ;
    :formalityLevelNumeric "3"^^xsd:integer ;
    :formalityGuidanceNotes "Comunicação profissional padrão. Usar frases completas. Evitar gíria excessivamente casual, mas informalidade técnica com colegas é aceitável."@pt .

:SGP_StudentBaseDefault rdf:type :StyleGuidanceProfile ;
    rdfs:label "Perfil de Orientação de Estilo Base do Estudante"@pt ;
    :includesToneElement :Tone_SemiFormalColaborativo ;
    :specifiesFormalityElement :Formality_Media ;
    :hasPrimaryCommunicationGoal :Goal_ProvideClearUpdate .

:Tone_RespeitosoFocadoTecnico rdf:type :ToneElement ;
    rdfs:label "Tom Respeitoso Focado Técnico"@pt ;
    :label "RespeitosoFocadoTecnico" ;
    :toneKeyword "manter o mais alto respeito"@pt, "orientado para tarefas"@pt, "conciso"@pt, "linguagem técnica precisa quando apropriado"@pt, "objetivo"@pt ;
    :avoidToneKeyword "casual"@pt, "especulativo a menos que claramente indicado"@pt, "emocional"@pt ;
    :toneIntensity "0.8"^^xsd:float .

:Formality_MediaAlta rdf:type :FormalityElement ;
    rdfs:label "Formalidade Média-Alta"@pt ;
    :label "MediaAlta" ;
    :formalityLevelNumeric "4"^^xsd:integer ;
    :formalityGuidanceNotes "Formal, mas permite a profundidade técnica necessária. Usar títulos ao dirigir-se. Garantir clareza e profissionalismo."@pt .

:SGP_StudentToProfInquiry rdf:type :StyleGuidanceProfile ;
    rdfs:label "Perfil de Estilo: Estudante para Professor (Questão)"@pt ;
    :includesToneElement :Tone_RespeitosoFocadoTecnico ;
    :specifiesFormalityElement :Formality_MediaAlta ;
    :hasPrimaryCommunicationGoal :Goal_RequestClarification .

# Perfil de Estilo Base para Professor
:Tone_ProfissionalConstrutivo rdf:type :ToneElement ;
    rdfs:label "Tom Profissional Construtivo"@pt ;
    :label "ProfissionalConstrutivo" ;
    :toneKeyword "claro e objetivo"@pt, "feedback construtivo"@pt, "profissional"@pt, "respeitoso"@pt, "encorajador quando apropriado"@pt ;
    :avoidToneKeyword "excessivamente vago"@pt, "depreciativo"@pt, "sarcástico"@pt ;
    :toneIntensity "0.7"^^xsd:float .

:Formality_ProfMediaAlta rdf:type :FormalityElement ; # Ligeiramente diferente para professor
    rdfs:label "Formalidade Profissional Média-Alta"@pt ;
    :label "ProfMediaAlta" ;
    :formalityLevelNumeric "4"^^xsd:integer ;
    :formalityGuidanceNotes "Comunicação profissional e académica. Clareza e precisão são essenciais. Adaptar ligeiramente a formalidade ao contexto do estudante, mas manter profissionalismo."@pt .

:SGP_ProfessorBaseDefault rdf:type :StyleGuidanceProfile ;
    rdfs:label "Perfil de Orientação de Estilo Base do Professor"@pt ;
    :includesToneElement :Tone_ProfissionalConstrutivo ;
    :specifiesFormalityElement :Formality_ProfMediaAlta ;
    :hasPrimaryCommunicationGoal :Goal_ProvideClearUpdate . # Pode partilhar objetivos ou ter outros específicos

# Perfil de Estilo Adaptado para Professor a dar Feedback a Estudante
:SGP_ProfessorToStudentFeedback rdf:type :StyleGuidanceProfile ;
    rdfs:label "Perfil de Estilo: Professor para Estudante (Feedback)"@pt ;
    :includesToneElement :Tone_ProfissionalConstrutivo ; # Reutiliza o tom base do professor
    :specifiesFormalityElement :Formality_Media ; # Pode ser ligeiramente menos formal que o base do prof.
    :hasPrimaryCommunicationGoal <http://www.semanticweb.org/ontologies/2025/5/academicEmailPersona#Goal_ProvideFeedback> . # Novo objetivo

:Goal_ProvideFeedback rdf:type :CommunicationGoal ;
    rdfs:label "Objetivo de Fornecer Feedback"@pt ;
    :label "ProvideFeedback" ;
    :goalDescription "O objetivo primário é fornecer feedback construtivo e claro sobre um trabalho, desempenho ou questão."@pt .


# --- Indivíduos: Arquétipos de Persona & Seus Atributos ---

:student_default_comm_attributes rdf:type :CommunicationAttributes;
    rdfs:label "Atributos de Comunicação Padrão do Estudante"@pt ;
    :language "pt-PT" ;
    :baseVerbosity "Equilibrado"@pt ;
    :baseSentenceStructure "ClaraConcisa"@pt ;
    :baseVocabularyPreference "AdequadoAoContexto"@pt ;
    :emojiUsage "raro_ou_nenhum_formal"@pt .

:student_guideline_do1 rdf:type :CommunicationGuideline ; rdfs:label "Estudante Do 1"@pt ; :guidelineText "Ser pontual e respeitar prazos."@pt .
:student_guideline_do2 rdf:type :CommunicationGuideline ; rdfs:label "Estudante Do 2"@pt ; :guidelineText "Comunicar de forma clara, concisa e respeitosa."@pt .
:student_guideline_do3 rdf:type :CommunicationGuideline ; rdfs:label "Estudante Do 3"@pt ; :guidelineText "Identificar-se devidamente (nome, número de aluno, curso) em comunicações formais."@pt .
:student_guideline_dont1 rdf:type :CommunicationGuideline ; rdfs:label "Estudante Don't 1"@pt ; :guidelineText "Usar linguagem excessivamente informal ou calão com docentes ou serviços."@pt .
:student_guideline_dont2 rdf:type :CommunicationGuideline ; rdfs:label "Estudante Don't 2"@pt ; :guidelineText "Exigir respostas imediatas, especialmente fora do horário de expediente."@pt .

:student_default rdf:type :StudentArchetype ;
    rdfs:label "Arquétipo Padrão de Estudante"@pt ;
    :archetype_description_note "Arquétipo base para um estudante (licenciatura, mestrado)."@pt ;
    :roleTemplate "Estudante de [NomeDoCurso] em [InstituicaoDeEnsino]" ;
    :description "Estudante dedicado(a) e organizado(a), focado(a) nos estudos e em atividades extracurriculares relevantes. Procura comunicar de forma clara e respeitosa, adaptando-se aos diferentes interlocutores no meio académico."@pt ;
    :hasCommunicationAttributes :student_default_comm_attributes ;
    :hasBaseStyleProfile :SGP_StudentBaseDefault ;
    :hasGeneralDo :student_guideline_do1, :student_guideline_do2, :student_guideline_do3 ;
    :hasGeneralDont :student_guideline_dont1, :student_guideline_dont2 .

:professor_default_comm_attributes rdf:type :CommunicationAttributes;
    rdfs:label "Atributos de Comunicação Padrão do Professor"@pt ;
    :language "pt-PT" ;
    :baseVerbosity "ObjetivoCompleto"@pt ;
    :baseSentenceStructure "BemEstruturadaClara"@pt ;
    :baseVocabularyPreference "TecnicoPreciso"@pt ;
    :emojiUsage "nenhum_formal"@pt .

:professor_guideline_do1 rdf:type :CommunicationGuideline ; rdfs:label "Professor Do 1"@pt ; :guidelineText "Ser claro(a) e objetivo(a) nas comunicações."@pt .
:professor_guideline_do2 rdf:type :CommunicationGuideline ; rdfs:label "Professor Do 2"@pt ; :guidelineText "Fornecer feedback construtivo e atempado."@pt .
:professor_guideline_dont1 rdf:type :CommunicationGuideline ; rdfs:label "Professor Don't 1"@pt ; :guidelineText "Ser excessivamente vago(a) ou ambíguo(a)."@pt .

:professor_default rdf:type :ProfessorArchetype ;
    rdfs:label "Arquétipo Padrão de Professor"@pt ;
    :archetype_description_note "Arquétipo base para um docente ou investigador universitário."@pt ;
    :roleTemplate "Professor(a) [Cargo] no Departamento de [NomeDepartamento] da [InstituicaoDeEnsino]" ;
    :description "Docente e investigador(a) experiente, dedicado(a) ao ensino e à orientação de estudantes, bem como à investigação científica. Comunica de forma clara, profissional e construtiva."@pt ;
    :hasCommunicationAttributes :professor_default_comm_attributes ;
    :hasBaseStyleProfile :SGP_ProfessorBaseDefault ;
    :hasGeneralDo :professor_guideline_do1, :professor_guideline_do2 ;
    :hasGeneralDont :professor_guideline_dont1 .

# --- Indivíduos: Regras Genéricas de Adaptação ao Destinatário ---

:rule_s2p_inquiry_do1 rdf:type :CommunicationGuideline ; rdfs:label "S2P Inquiry Do 1"@pt ; :guidelineText "Indicar claramente a unidade curricular (se aplicável) e o assunto."@pt .
:rule_s2p_inquiry_dont1 rdf:type :CommunicationGuideline ; rdfs:label "S2P Inquiry Don't 1"@pt ; :guidelineText "Fazer perguntas genéricas como 'Não percebi nada'."@pt .

:rule_student_to_professor_academic_inquiry rdf:type :GenericRecipientAdaptationRule ;
    rdfs:label "Regra: Estudante para Professor (Questão Académica)"@pt ;
    :ruleKey "student_to_professor_academic_inquiry" ;
    :description "Estudante contacta professor para esclarecer dúvidas académicas (matéria, avaliação, etc.)."@pt ;
    :greetingTemplate "Estimado(a) Professor(a) [ApelidoProfessor]," ;
    :farewellTemplate "Com os melhores cumprimentos,\n[NomeCompletoAluno]\nNº [NumeroAluno]\n[NomeCurso]" ;
    :hasAdaptedStyleProfile :SGP_StudentToProfInquiry ;
    :hasSpecificDo :rule_s2p_inquiry_do1 ;
    :hasSpecificDont :rule_s2p_inquiry_dont1 .

:rule_p2s_feedback_do1 rdf:type :CommunicationGuideline ; rdfs:label "P2S Feedback Do 1"@pt ; :guidelineText "Ser específico sobre os pontos positivos e áreas a melhorar."@pt .
:rule_p2s_feedback_dont1 rdf:type :CommunicationGuideline ; rdfs:label "P2S Feedback Don't 1"@pt ; :guidelineText "Focar apenas nos aspetos negativos."@pt .

:rule_professor_to_student_feedback rdf:type :GenericRecipientAdaptationRule ;
    rdfs:label "Regra: Professor para Estudante (Feedback Construtivo)"@pt ;
    :ruleKey "professor_to_student_feedback_constructive" ; # Chave do JSON original
    :description "Professor fornece feedback sobre trabalho, desempenho ou projeto de um estudante."@pt ;
    :greetingTemplate "Caro(a) [NomeAluno]," ;
    :farewellTemplate "Com os melhores cumprimentos,\n[NomeProfessor]\n[CargoProfessor]" ;
    :hasAdaptedStyleProfile :SGP_ProfessorToStudentFeedback ;
    :hasSpecificDo :rule_p2s_feedback_do1 ;
    :hasSpecificDont :rule_p2s_feedback_dont1 .


# --- Exemplo de Indivíduos para Ideia 3: Conhecimento Aprendido (estrutura como antes) ---

:category_feedbackRevisaoDocs rdf:type :AcademicEmailCategory;
    rdfs:label "Categoria Revisão de Documentos para Feedback"@pt;
    :categoryID "FEEDBACK_REVISAO_DOCS";
    :displayName "Feedback, Revisão e Partilha de Documentos"@pt;
    :description "Emails relacionados com a revisão de artigos, relatórios, código, pedidos de feedback."@pt .

:ics_feedback_001 rdf:type :InteractionContextSnapshot ;
    rdfs:label "Contexto da Interação para Feedback 001"@pt ;
    :snapshotForPersona :student_default ;
    :snapshotRecipientCategoryKey "prof_jorge" ;
    :snapshotSenderNameGuess "Jorge Bernardino" ;
    :snapshotIncomingTone "Formal"@pt ;
    :snapshotOriginalEmailTextSnippet "Caro Rodrigo, agradeço que reveja o documento em anexo..."@pt ;
    :snapshotUserGuidanceInputs "[{\"point\": \"Rever documento\", \"guidance\": \"Confirmar revisão e indicar prazo\"}]" ;
    :snapshotIAActionType "/draft" ;
    :snapshotLLMPreAnalysisRationale "Remetente identificado como 'Professor Jorge Bernardino' na lista de relações."@pt ;
    :snapshotIdentifiedEmailCategory :category_feedbackRevisaoDocs .

:lk_item_001 rdf:type :LearnedKnowledgeItem ;
    rdfs:label "Item Aprendido 001 - Saudação Prof Jorge"@pt ;
    :hasTimestampUTC "2025-06-05T02:00:00Z"^^xsd:dateTime ;
    :hasFeedbackCategory "PREFERENCIA_SAUDACAO"@pt ;
    :aiOriginalResponseText "Prezado Professor Jorge," ;
    :userCorrectedOutputText "Estimado Professor Jorge Bernardino," ;
    :userExplanationText "Para o Prof. Jorge, prefiro usar 'Estimado' e o nome completo na saudação inicial."@pt ;
    :modelUsedForOriginal "gemini-1.5-flash-latest" ;
    :hasInteractionContext :ics_feedback_001 .

:student_default :hasLearnedItem :lk_item_001 .

# --- Indivíduos: Predefinições de Processo de IA & Categorias de Email Académico ---

:option_handleMissingInfo_requestUserInput rdf:type :IAProcessOption ;
    rdfs:label "Opção Info em Falta: Pedir Input"@pt ;
    :value "ALWAYS_REQUEST_USER_INPUT_FIRST" ;
    :description "IA sempre sinaliza à Persona a necessidade de input específico se suspeitar que falta info crítica."@pt ;
    :options_description """LLM_BEST_EFFORT_WITH_EMAIL_ONLY: IA tenta responder da melhor forma apenas com o email original.
PRIORITIZE_USER_DIRECT_INPUT_THEN_CAUTIOUS_SHORT_RESPONSE: IA prioriza input direto da Persona. Se ausente e info crítica parece faltar, IA gera resposta curta e cautelosa.
ALWAYS_REQUEST_USER_INPUT_FIRST: IA sempre sinaliza à Persona a necessidade de input específico antes de gerar um rascunho completo se suspeitar que falta info crítica."""@pt .

:option_priorityUserInput_veryHigh rdf:type :IAProcessOption ;
    rdfs:label "Opção Prioridade Input Utilizador: Muito Alta"@pt ;
    :value "VERY_HIGH" ;
    :description "Nível de importância a dar ao input explícito da Persona face à análise do email original."@pt ;
    :options_description "LOW, MODERATE, HIGH, VERY_HIGH"@pt .

:option_expectedAutonomy_suggestAndAwait rdf:type :IAProcessOption ;
    rdfs:label "Opção Autonomia IA: Sugerir e Aguardar"@pt ;
    :value "SUGGEST_ACTIONS_AND_AWAIT_GUIDANCE" ;
    :description "Nível de proatividade esperado da IA em sugerir elaborações, ações não explícitas, etc."@pt ;
    :options_description """MINIMAL_AUTONOMY_STICK_TO_RULES_ONLY
SUGGEST_ACTIONS_AND_AWAIT_GUIDANCE
PROACTIVE_ELABORATION_WITH_CONFIRMATION_NEEDED"""@pt .

:option_unclearRequests_requestClarification rdf:type :IAProcessOption ;
    rdfs:label "Opção Pedidos Pouco Claros: Pedir Clarificação"@pt ;
    :value "REQUEST_CLARIFICATION_FROM_USER" ;
    :description "Como a IA deve proceder se partes do email do remetente forem ambíguas."@pt ;
    :options_description """LLM_BEST_EFFORT_WITH_EMAIL_ONLY: IA tenta responder da melhor forma apenas com o email original.
REQUEST_CLARIFICATION_FROM_USER: IA pede à Persona para clarificar a ambiguidade antes de tentar uma resposta.
OFFER_MULTIPLE_INTERPRETATIONS_TO_USER: IA apresenta possíveis interpretações da ambiguidade à Persona para que esta escolha."""@pt .

:global_ia_defaults rdf:type :IAProcessDefaultSet ;
    rdfs:label "Predefinições Globais de Processo de IA"@pt ;
    :definesHandlingOfMissingInfo :option_handleMissingInfo_requestUserInput ;
    :definesPriorityOfUserInput :option_priorityUserInput_veryHigh ;
    :definesExpectedIAAutonomyLevel :option_expectedAutonomy_suggestAndAwait ;
    :definesBehaviorForUnclearRequests :option_unclearRequests_requestClarification .

:category_courseAdminQueries rdf:type :AcademicEmailCategory ;
    rdfs:label "Categoria Questões Administrativas de Curso"@pt ;
    :categoryID "COURSE_ADMINISTRATION_QUERIES" ;
    :displayName "Questões Administrativas de Curso/UC"@pt ;
    :description "Emails sobre inscrições, horários, salas, avaliações, notas, pautas, prazos administrativos."@pt .

:category_lectureMaterialClarification rdf:type :AcademicEmailCategory;
    rdfs:label "Categoria Esclarecimento Matéria Letiva"@pt;
    :categoryID "LECTURE_MATERIAL_CLARIFICATION";
    :displayName "Esclarecimento sobre Matéria Letiva"@pt;
    :description "Perguntas sobre o conteúdo das aulas, slides, bibliografia recomendada."@pt .

:category_projectSupervisionUpdatesMeetings rdf:type :AcademicEmailCategory;
    rdfs:label "Categoria Supervisão Projeto (Updates, Reuniões)"@pt;
    :categoryID "PROJECT_SUPERVISION_UPDATES_MEETINGS";
    :displayName "Supervisão de Projeto/Tese (Updates, Reuniões)"@pt;
    :description "Comunicações entre orientador e orientado sobre progresso, dificuldades, agendamento de reuniões de orientação."@pt .

:global_ia_defaults :definesEmailCategory :category_courseAdminQueries, :category_lectureMaterialClarification, :category_projectSupervisionUpdatesMeetings, :category_feedbackRevisaoDocs .
