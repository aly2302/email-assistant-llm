#################################################################
#    Metadados da Ontologia
#################################################################

<http://www.semanticweb.org/ontologies/2025/5/academicEmailPersona> rdf:type owl:Ontology ;
    owl:versionIRI <http://www.semanticweb.org/ontologies/2025/5/academicEmailPersona/3.0.0-pt> ;
    rdfs:label "Ontologia de Personas para Email Académico (V3 - Final)"@pt ;
    rdfs:comment """Versão final e refatorizada da meta-ontologia.
Esta versão V3.0.0 introduz uma estrutura modular com a classe PersonaConfiguration para melhorar a organização visual e lógica.
Remove a classe CommunicationGoal, confiando no LLM para inferir o objetivo.
Distingue claramente entre Arquétipos (classes e indivíduos modelo) e Personas Específicas (indivíduos de produção)."""@pt ;
    owl:versionInfo "3.0.0-pt" .

#################################################################
#    Propriedades de Anotação
#################################################################

:schema_version rdf:type owl:AnnotationProperty ; rdfs:label "Versão do Esquema Original"@pt .
:notes_on_structure rdf:type owl:AnnotationProperty ; rdfs:label "Notas sobre a Estrutura"@pt .
:archetype_description_note rdf:type owl:AnnotationProperty ; rdfs:label "Nota de Descrição do Arquétipo"@pt .
:options_description rdf:type owl:AnnotationProperty ; rdfs:label "Descrição das Opções"@pt .
:applies_from_archetype_note rdf:type owl:AnnotationProperty ; rdfs:label "Nota de Aplicação a Partir do Arquétipo"@pt .
:applies_to_recipient_category_note rdf:type owl:AnnotationProperty ; rdfs:label "Nota de Aplicação à Categoria de Destinatário"@pt .

#################################################################
#    Classes
#################################################################

:PersonaArchetype rdf:type owl:Class ; rdfs:label "Arquétipo de Persona"@pt .
